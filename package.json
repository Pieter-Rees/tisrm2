{
  "name": "tisrm",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:optimized": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true NODE_ENV=production next build",
    "build:turbo": "next build --turbo",
    "build:static": "NEXT_EXPORT=true next build",
    "export": "npm run build:static && next export",
    "export:static": "npm run build:static && next export -o static",
    "start": "next start -p 3002",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next build static",
    "pre-commit": "npm run type-check && npm run lint && npm run format:check",
    "check:deps": "npx depcheck",
    "security:audit": "npm audit --audit-level moderate"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.24.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@next/third-parties": "^15.1.6",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "next": "^15.1.6",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.52.0",
    "react-icons": "^5.5.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.24.2",
    "@next/bundle-analyzer": "^15.5.3",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "critters": "^0.0.23",
    "cssnano": "^7.1.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.3.1",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.7.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  }
}
